								<!-- The temporalNameRange allows to specify multiple -->
								<!-- temporal name items where each contains a name of -->
								<!-- a range in time. -->
								<!-- </xs:documentation> -->
							    <!-- </xs:annotation> -->
							<!-- <xs:complexType> -->
							    <!-- <xs:sequence> -->
								<!-- <xs:element name="temporalNameItem" minOccurs="1" maxOccurs="unbounded"> -->
