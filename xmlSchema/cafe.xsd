														    <xs:element name="bathyal" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
														    <xs:element name="abyssal" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
														    <xs:element name="hadal" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
														</xs:sequence>
													    </xs:complexType>
													</xs:element>
													<xs:element name="pelagic" minOccurs="0" maxOccurs="unbounded">
													    <xs:complexType>
														<xs:sequence>
														    <xs:element name="epipelagic" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
														    <xs:element name="mesopelagic" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
														    <xs:element name="bathypelagic" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
														    <xs:element name="abyssopelagic" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
