<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="cafeTohtml.xsl"?>

<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
    version="0.0.1"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:sawsdl="http://www.w3.org/ns/sawsdl">

    <xs:element name="cafe">
	<xs:complexType>
	    <xs:sequence>
		<xs:element name="physical" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The physical element captures important information on the
			    referenced search object that is classified using the other
			    elements of the schema.
			</xs:documentation>
		    </xs:annotation>

		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="object" minOccurs="1" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="objectType" type="ObjectType" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The type of object
						    referenced with this
						    annotation. This could be
						    audio, image, video,
						    dataset.
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
					<xs:element name="objectName" type="xs:string" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The full filename of the
						    object with extension
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
					<xs:element name="objectReferenceUrl" type="xs:string" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The URL for reference. Can
						    be used to link to more
						    metadata or a homepage with
						    more details and from where
						    the download of the data is
						    reachable.
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
					<xs:element name="objectDownloadUrl" type="xs:string" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    This to set the download
						    URL of the data.
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="time" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The time facet covers all aspects of time. It
			    captures time as point or as ranges and allows to
			    specify a temporal granularity.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="temporalCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The temporal coverage module captures
					the numeric aspects of dates in iso 8601
					format. It allows to either specify a
					time point or a time range.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:choice>
					<xs:sequence>
					    <xs:element name="dateTimePoint" minOccurs="1" maxOccurs="1">
						<xs:annotation>
						    <xs:documentation xml:lang="en">
							The dateTimePoint
							allows to specify a
							point in time via
							seperate date and time
							fields or via a
							combined date field.
							Both need to be conform
							to the iso 8601
							standard.
						    </xs:documentation>
						</xs:annotation>
						<xs:complexType>
						    <xs:sequence>
							<xs:element name="datePoint" type="xs:date" minOccurs="1" maxOccurs="1">
							    <xs:annotation>
								<xs:documentation xml:lang="en">
								    A single date in form
								    YYYY-MM-DD or with an offset from UTC with
								    conform +XX:XX appende conform to
								    to iso 8601.
								</xs:documentation>
							    </xs:annotation>
							</xs:element>
							<xs:element name="timePoint" type="xs:time" minOccurs="1" maxOccurs="1">
							    <xs:annotation>
								<xs:documentation xml:lang="en">
								    A single time in form
								    HH:MM:SS or with an offset from UTC with
								    +XX:XX appended conform to iso 8601.
								</xs:documentation>
							    </xs:annotation>
							</xs:element>
						    </xs:sequence>
						</xs:complexType>
					    </xs:element>
					</xs:sequence>
					<xs:sequence>
					    <xs:element name="dateTimeRange" minOccurs="1" maxOccurs="1">
						<xs:annotation>
						    <xs:documentation xml:lang="en">
							The dateTimeRange
							allows to specify one
							or multiple ranges in
							time via seperate date
							and time fields or via
							a combined date field.
							Both need to be conform
							to the iso 8601
							standard.
						    </xs:documentation>
						</xs:annotation>
						<xs:complexType>
						    <xs:sequence>
							<xs:element name="dateTimeItem" minOccurs="1" maxOccurs="unbounded">
							    <xs:complexType>
								<xs:sequence>
								    <xs:element name="dateStart" type="xs:date" minOccurs="1" maxOccurs="1">
									<xs:annotation>
									    <xs:documentation xml:lang="en">
										A single date in form
										YYYY-MM-DD or with an offset from UTC with
										conform +XX:XX appende conform to
										to iso 8601.
									    </xs:documentation>
									</xs:annotation>
								    </xs:element>
								    <xs:element name="timeStart" type="xs:time" minOccurs="1" maxOccurs="1">
									<xs:annotation>
									    <xs:documentation xml:lang="en">
										A single time in form
										HH:MM:SS or with an offset from UTC with
										+XX:XX appended conform to iso 8601.
									    </xs:documentation>
									</xs:annotation>
								    </xs:element>
								    <xs:element name="dateEnd" type="xs:date" minOccurs="1" maxOccurs="1">
									<xs:annotation>
									    <xs:documentation xml:lang="en">
										A single date in form
										YYYY-MM-DD or with an offset from UTC with
										conform +XX:XX appende conform to
										to iso 8601.
									    </xs:documentation>
									</xs:annotation>
								    </xs:element>
								    <xs:element name="timeEnd" type="xs:time" minOccurs="1" maxOccurs="1">
									<xs:annotation>
									    <xs:documentation xml:lang="en">
										A single time in form
										HH:MM:SS or with an offset from UTC with
										+XX:XX appended conform to iso 8601.
									    </xs:documentation>
									</xs:annotation>
								    </xs:element>
								</xs:sequence>
							    </xs:complexType>
							</xs:element>
						    </xs:sequence>
						</xs:complexType>
					    </xs:element>
					</xs:sequence>
				    </xs:choice>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="temporalNamespace" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The temporal namespace captures named
					times like the geological ages from the
					international chronostratigraphic chart
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="temporalNameRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The temporal name range
						    allows to specify multiple
						    items where each contains a
						    name and a reference to a
						    standard.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="temporalNameItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The temporal name item captures one temporal name each and a specification
								of a standard where the name comes from.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="temporalName" type="TemporalName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The temporal name covers names of times like for example
									    the geological time scale (Hadean, Archean, Proterozoic,
									    Phanerozoic)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="isLowerBoundary" type="xs:boolean" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Designates if the item is the lower boundary of a time range.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="isUpperBoundary" type="xs:boolean" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Designates if the item is the upper boundary of a time range.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="temporalStandard" type="TemporalStandard" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The temporal standard is a reference from where the names of times
									    comes from e.g. the international chronostratigraphic chart
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="temporalRaster" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The temporal raster allows to span a categorial grid in time with the extent being the frame
					and time resolution the subdivision.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="temporalExtent" type="TemporalClass" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    Defines the extent in time in form of a category (second, minute, hour ...).
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
					<xs:element name="temporalResolution" type="TemporalClass" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    Defines the resolution in time in form of a category (second, minute, hour ...).
						    This should be smaller than the extent to be meaningful.
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="space" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The space facet covers all localization aspects.
			    The information ranges from location names over the
			    specification of the country or continent down to
			    the extent and resolution of the data.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="spatialCoverage" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The spatial coverage deals with
					capturing the numeirc aspects of
					location forming a bounding box with
					corrdinates and the extent of
					elevation.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="boundingBox" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The bounding box is a three dimensional virtual object in space
						    that is made up by the four coordinates (N,E,S,W) in form of decimal
						    degree as well as the maximum and minimum altitude (reference sealevel).
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="boundingCoordinate" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The bounding coordinate contains the geographic coordinates to frame the
								location.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <!-- it would be a good idea to include gps points or sub bounding boxes in the bounding box as sub category -->
							    <!-- this would cpature granularity and extent in a more numeric way compared to the way we do it in the raster -->
							    <xs:sequence>
								<xs:element name="northBoundingCoordinate" type="NorthSouthBoundingCoordinate" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The north most geographic coordinate that signifies the boundary of the data. It
									    has to be given in decimal degree between -90 and +90 degree.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="southBoundingCoordinate" type="NorthSouthBoundingCoordinate" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The soutch most geographic coordinate that signifies the boundary of the data. It
									    has to be given in decimal degree between -90 and +90 degree.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="eastBoundingCoordinate" type="EastWestBoundingCoordinate" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The west most geographic coordinate that signifies the boundary of the data. It
									    has to be given in decimal degree between -180 and +180 degree.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="westBoundingCoordinate" type="EastWestBoundingCoordinate" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The west most geographic coordinate that signifies the boundary of the data. It
									    has to be given in decimal degree between -180 and +180 degree.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						    <xs:element name="boundingElevation" minOccurs="1" maxOccurs="1">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="maximumElevation" type="xs:decimal" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The maximal depth in case the data is located below sealevel.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="maximumElevationUnit" type="LengthUnit" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The maximal depth unit (e.g. meter, centimeter)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="minimumElevation" type="xs:decimal" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The minimal depth in case the data is located below sealevel.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="minimumElevationUnit" type="LengthUnit" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The minimal depth unit (e.g. meter, centimeter)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="elevationDatum" type="xs:string" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The reference datum (defaults to meanSealevel)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
					<xs:element name="siteCoordinate" minOccurs="0" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="siteCoordinateRange" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="siteCoordinateItem" minOccurs="1" maxOccurs="unbounded">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="utmCoordinate" minOccurs="1" maxOccurs="1">
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="zone" type="xs:integer" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The numerical zone corresponding to the central meridian
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="subZone" type="xs:integer" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The subzone letter corresponding to one of the 20 north-south divisions of the
												    UTM grid system
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="northSouth" type="NorthSouth" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The hemisphere. This can be N (North) or S (South)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="easting" type="xs:decimal" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The distance east of the origin of the UTM zone
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="eastingUnit" type="LengthUnit" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The unit of the distance of the easting (meter, kilometer)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="northing" type="xs:decimal" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The distance north of the origin of the UTM zone
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="northingUnit" type="LengthUnit" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The unit of the distance of the northing (meter, kilometer)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="geodeticDatum" type="GeodeticDatum" minOccurs="0" maxOccurs="unbounded">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The datum e.g. wgs84
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="spatialNamespace" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The spatial namespace captures named
					places and the type of the references
					e.g. Leipzig/City.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="spatialNameRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The spatial name range captures a range of location names and types
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="spatialNameItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The spatial name item covers a range of spatial names and spatial types
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="spatialName" type="SpatialName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The name of the location (Leipzig, Cospudener See)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="spatialType" type="SpatialType" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The type of the location. This could be e.g. City, Lake, etc.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="spatialRaster" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The spatial raster allows to span a categorial grid in space with the extent being the frame
					and time resolution the subdivision.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="spatialExtent" type="SpatialSizeClass" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    Defines the extent in space in form of a category (point, plot, region, continent, globa).
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
					<xs:element name="spatialResolution" type="SpatialSizeClass" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    Defines the resolution in space in form of a category (point, plot, region, continent, globa).
						    This should be smaller than the extent to be meaningful.
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="method" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The method facet covers all aspects about the
			    methodological approach and the factors that have
			    been modified or observed as covariates.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="methodologicalCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The methodological coverage captures the methodological approach as
					weel at the method type.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="generalApproach" type="GeneralApproach" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The general approach that thas been used e.g. virtual experiment, controlled experiment,
						    field experiment, field manipulation, mere observation.
						</xs:documentation>
					    </xs:annotation>
					</xs:element>
					<xs:element name="controlFactorRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    This element covers the range of control factors that are
						    involved and if the have been manipulated or just observed
						    as covariates.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="controlFactorItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								This element covers the range of control factors that have
								been manipulated or just observed
								as covariates.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="controlFactorName" type="ControlFactorName" minOccurs = "1" maxOccurs = "1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The name of the control factor that has been measured or observed
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="controlFactorModus" type="ControlFactorModus" minOccurs = "1" maxOccurs = "1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The modus of the control factor. It can be manipulated or observed
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="controlFactorEnvironmentalDependency" type="ControlFactorEnvironmentalDependency" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Captures the degree of the environmental dependency of the factor: independent, partial dependent, fully dependent.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="sphere" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The sphere module deals with aspects of localization
			    and thus complements the space module. It allows to
			    more fine granular adjust the location within
			    the bounding box that has been given.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="sphericalCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The spherical coverage allows to specify the range of spheres that
					is involved.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="sphereRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The sphere range captures a range of sphere items with name and sub spheres.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="sphereItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The sphere item captures a sphere item with name and sub spheres.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="sphereName" type="SphereName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The sphere item captures a sphere item with name and sub spheres.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="zoneCoverage" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="zoneRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The zone range captures a range of spheres, zones and layers under
						    the main spheres selected in the annotation.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="zoneItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The zone item captures a sphere, zone or layer item by name.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="zoneName" type="ZoneName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The zone name captures a sphere, zone or layer item by name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="biome" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The biome facet covers the detailed description of
			    the biome the data originates from. The discription
			    ranges from the name of the biome over its type and
			    the latitudinal zonation as well as it covers the
			    hydrological aspects describing the biome.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="biomicCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The biomic coverage captures biomes by
					name, tpye, the latitudianal zonataion
					and water availablility.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="biomeRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The biome range allows to add multiple biomes.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="biomeItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The biome item captures the
								name, the tpye, the latitudianal zonataion
								and water availablility of a specific biome.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<!-- <xs:element name="biomeName" type="xs:string" minOccurs="0" maxOccurs="1"/> -->
								<xs:element name="biomeType" type="BiomeType"  minOccurs="1" maxOccurs="1">
								    <!-- biome type is not a good word -->
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The biome type captures the general type of the biome e.g. terrestrial,
									    aquatic, wetland, dryland, ecotone, fresh water, brakish water, saline water
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="isEcotone" type="IsEcotone"  minOccurs="1" maxOccurs="1">
								    <!-- biome type is not a good word -->
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Allows to specify weather the biome was an ecotone or not.
									    This can take the values True and False.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="biomeZonation" type="BiomeZoneation" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The biome zone caputres the latitudinal zone e.g. polar, boreal, temperate
									    subtropical, tropical
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="biomeHemisphere" type="BiomeHemisphere" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    This captures the hemisphere the biome is in. This can take the
									    values Northern or Southern.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="biomeWater" type="BiomeWater" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The water availablility is captured here. This can take the values
									    Arid, Semi Arid, Semi Humid, Humid.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
					<xs:element name="physiognomyRange" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The physiognomy range caputres the range of available physiognomies
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="physiognomyItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The physiognomy item caputres a specific type of physiognomy by name and type
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="physiognomyClass" type="PhysiognomyClass" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The physiognomy class captures the broad classification of the physiognomy (forest,
									    open woodland, shrubland ...)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="physiognomyType" type="PhysiognomyType" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The physiognomy type details the classification of the physiognomy (e.g.
									    class: forest, type mixed).
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="landUseRange" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The land use range allows to specify the range of land uses
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="landUseItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The land use item allows to specify a specific the land with a
						    broad classification and to give the purpose the land is used for.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="landUseClass" type="LandClass" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								Allows a broad classification of the land in the observation area
								in terms of naturality (natural, semi natural, urban).
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>
						    <xs:element name="landUsePurpose" type="LandUsePurpose" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								Allows to specify the purpose the land in the observation area is used
								for (agriculture, aquaculture, agroforestry, vegetated)
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="organism" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The organism facet covers aspects of organisms
			    description. The description ranges from life forms
			    the size of the organisms and a taxonomical
			    classification.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="organismicCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The organismic coverage deals with aspects of taxonomically classified
					organisms as well as with the characterization of life forms.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="organismRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The organismic range allows to specify multiple taxonomically
						    classified organisms.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="organismItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The organism item allows to capture a specific organisms by taxonomical
								classification.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="taxonomicClassification" type="TaxonomicClassification" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The taxonomic rank name specifies the level of the taxonomic naming given
									    in the taxonomicName field in the organism item. This can be e.g.
									    domain, kingdom, phylum, division, class, order, family, genus, species
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="organismName" type="OrganismName" minOccurs="0" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Atomised names of the taxon, with subtypes according to the
									    different Codes of Nomenclature which apply to scientific
									    names.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="lifeFormCoverage" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="lifeFormRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The life form range allows to specify multiple types of life forms
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="lifeFormItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The life form item allows to specify a specific life form
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="lifeFormClass" type="LifeFormClass" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    A broad general classification of the life form (animal, plant, bacteria, virus)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="lifeFormType" type="LifeFormType" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Allows further specification of the life form covering various aspects (hydrophyte,
									    homöotherm ...).
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="organismSizeCoverage" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="organismSizeRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The organism size range reflects the size of orgaisms involved in the observation.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="organismSizeItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The organism size reflects the maximum and minimum size of organism in the observation.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="organismSizeMinimum" type="NonNegativeDecimal" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The minimum size of an organism
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="organismSizeMinimumUnit" type="LengthUnit" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The unit of the minimum size of an organism  (Pico, Nano, Micro, ...Metre)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="organismSizeMaximum" type="NonNegativeDecimal" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The maximum size of an organism
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="organismSizeMaximumUnit" type="LengthUnit" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The unit of the maximum size of an organism  (pico, nano, micro, ...Metre)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="organismSizeClass" type="OrganismSizeClass" minOccurs="0" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Providesa a classification approach to the size information (micro, meso, macroFlora, microFauna ...)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="chemical" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The chemical facet covers all information about
			    chemical elements, compounds or ions measured or
			    involved in the dataset.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="chemicalCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The chemical coverage module deals with
					aspects of elements and compounds that
					have been involved in the study and
					their classification.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="elementRange" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The chemical element range allows to specify elements
						    that have been involved in the study.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="elementItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The chemical element item allows to specify an element
								that has been involved in the study.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="elementName" type="ElementName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Specify a chemical element by name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
					<xs:element name="isotopeRange" minOccurs="0" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="isotopeItem" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="isotopeName" type="IsotopeName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Specify an isotope by name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
					<xs:element name="compoundRange" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The compound range allows to capture a range of chemical compound items
						    by their name and classification.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="compoundItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The compound item allows to caputre a specific chemical compund
								by name and classification.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="compoundName" type="xs:string" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Caputures the compound name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="compoundClass" type="CompoundClass" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Caputres the compound class (organic, inorganic)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>
								<xs:element name="compoundCategoryRange" minOccurs="0" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Allows to capture a range of categories the compound belong to
									</xs:documentation>
								    </xs:annotation>
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="compoundCategoryItem" minOccurs="1" maxOccurs="unbounded">
										<xs:annotation>
										    <xs:documentation xml:lang="en">
											The compound category item captures one specific compound categorization.
										    </xs:documentation>
										</xs:annotation>
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="compoundCategoryName" type="CompoundCategory" minOccurs="1" maxOccurs="1">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The compound category captures one compound categorization by name (e.g. nucleobase,
												    nucleotide, arene, alkene, alcohol)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="chemicalFunctionRange" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The chemical function range allows to capture a range of chemical functions
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="chemicalFunctionItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The chemical function item allows to capture one chemical function
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="chemicalFunctionName" type="chemicalFunctionNameType" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The chemical function name item allows to capture one chemical function (pesticide, enzyme,
								pigment, poison)
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="process" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The process facet deals with aspects about processes observed in the study. It covers the name of the
			    process and allows a classification of the process with process types.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="processCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The process coverage deals with capturing processes and their classification.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="processItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The process item covers a specific process by name and classifiction.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="processName" type="ProcessName" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The process name fields covers the process name.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>
						    <xs:element name="processClass" type="ProcessClass" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The process class covers the general character of the process. It
								can be interaction, transformation or transport.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>
						    <xs:element name="processType" type="ProcessType" minOccurs="0" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The process type covers a more detailed classification of the process.
								It goes hand in hand with the process class.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			    <xs:element name="interactionRange" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="interactionItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="firstInteractionPartner" type="InteractionPartner" minOccurs="1" maxOccurs="1"/>
						    <xs:element name="directionOfInteraction" type="DirectionOfInteraction" minOccurs="1" maxOccurs="1"/>
						    <xs:element name="secondInteractionPartner" type="InteractionPartner" minOccurs="1" maxOccurs="1"/>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>
			</xs:sequence>
		    </xs:complexType>
		</xs:element>
	    </xs:sequence>
	</xs:complexType>
    </xs:element>

