    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LandUseType">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	A type of land use
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Natural">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Natural is every land that is not managed in any way.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Semi Natural">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Semi natural is every land that is managed or used for food creating other
	    resources.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Urban">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Areas with human settlement.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LandUseForm">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	A type of land use form
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
