											<xs:sequence>
											    <xs:element name="zone" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
												    <xs:sequence>
													<xs:element name="benthic" minOccurs="0" maxOccurs="unbounded">
													    <xs:complexType>
														<xs:sequence>
														    <xs:element name="litoral" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
