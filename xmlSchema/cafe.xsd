														<xs:sequence>
														    <xs:element name="sandPercentage" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
														    <xs:element name="loamPercentage" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
														    <xs:element name="siltPercentage" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
														</xs:sequence>
													    </xs:complexType>
													</xs:element>
												    </xs:sequence>
												</xs:complexType>
											    </xs:element>

											</xs:sequence>
										    </xs:complexType>
										</xs:element>
									    </xs:sequence>
									</xs:complexType>
								    </xs:element>

								    <xs:element name="biosphere" minOccurs="0" maxOccurs="1">
									<xs:complexType>
									    <xs:sequence>
										<xs:element name="layer" minOccurs="0" maxOccurs="unbounded">
										    <xs:complexType>
											<xs:sequence>
											    <xs:element name="qualitative" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
												    <xs:sequence>
													<xs:element name="treeLayer" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="shrubLayer" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="herbLayer" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="mossLayer" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="groundLayer" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
												    </xs:sequence>
												</xs:complexType>
											    </xs:element>
											</xs:sequence>
										    </xs:complexType>
										</xs:element>

										<xs:element name="height" minOccurs="0" maxOccurs="1">
										    <xs:complexType>
											<xs:sequence>
											    <xs:element name="quantitative" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
												    <xs:sequence>
													<xs:element name="minimumValue" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
													<xs:element name="maximumValue" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
													<xs:element name="unit" type="LengthUnit" minOccurs="1" maxOccurs="1"/>
												    </xs:sequence>
												</xs:complexType>
											    </xs:element>
											</xs:sequence>
										    </xs:complexType>
										</xs:element>

										<xs:element name="organisation" minOccurs="0" maxOccurs="1">
										    <xs:complexType>
											<xs:sequence>
											    <xs:element name="biome" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="ecosystem" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="population" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="organism" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="sytemOfOrgans" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="organ" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="tissue" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="cell" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="cellOrganelle" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="molecule" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											    <xs:element name="atom" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
											</xs:sequence>
										    </xs:complexType>
										</xs:element>
									    </xs:sequence>
									</xs:complexType>
								    </xs:element>

								    <xs:element name="lithosphere" type="xs:boolean" minOccurs="0" maxOccurs="1"/>

								</xs:choice>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			    <xs:element name="zoneCoverage" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="zoneRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The zone range captures a range of spheres, zones and layers under
						    the main spheres selected in the annotation.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="zoneItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The zone item captures a sphere, zone or layer item by name.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="zoneName" type="ZoneName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The zone name captures a sphere, zone or layer item by name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="biome" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The biome facet covers the detailed description of
			    the biome the data originates from. The discription
			    ranges from the name of the biome over its type and
			    the latitudinal zonation as well as it covers the
			    hydrological aspects describing the biome.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>

			    <xs:element name="biomicCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The biomic coverage captures biomes by
					name, tpye, the latitudianal zonataion
					and water availablility.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="biomeRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The biome range allows to add multiple biomes.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="biomeItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The biome item captures the
								name, the tpye, the latitudianal zonataion
								and water availablility of a specific biome.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="biomeType" type="BiomeType"  minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The biome type captures the general type of the biome e.g. terrestrial,
									    aquatic, wetland, dryland, ecotone, fresh water, brakish water, saline water
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="biomeZonation" type="BiomeZoneation" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The biome zone caputres the latitudinal zone e.g. polar, boreal, temperate
									    subtropical, tropical
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="biomeHemisphere" type="BiomeHemisphere" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    This captures the hemisphere the biome is in. This can take the
									    values Northern or Southern.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="biomeHydrology" type="BiomeHydrology" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The water availablility is captured here. This can take the values
									    Arid, Semi Arid, Semi Humid, Humid.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="isEcotone" type="xs:boolean"  minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Allows to specify weather the biome was an ecotone or not.
									    This can take the values true and false.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>

					<xs:element name="physiognomyRange" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The physiognomy range caputres the range of available physiognomies
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="physiognomyItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The physiognomy item caputres a specific type of physiognomy by name and type
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="physiognomyClass" type="PhysiognomyClass" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The physiognomy class captures the broad classification of the physiognomy (forest,
									    open woodland, shrubland ...)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="physiognomyType" type="PhysiognomyType" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The physiognomy type details the classification of the physiognomy (e.g.
									    class: forest, type mixed).
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			    <xs:element name="landUseRange" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The land use range allows to specify the range of land uses
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="landUseItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The land use item allows to specify a specific the land with a
						    broad classification and to give the purpose the land is used for.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="landUseClass" type="LandClass" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								Allows a broad classification of the land in the observation area
								in terms of naturality (natural, semi natural, urban).
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						    <xs:element name="landUsePurpose" type="LandUsePurpose" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								Allows to specify the purpose the land in the observation area is used
								for (agriculture, aquaculture, agroforestry, vegetated)
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="organism" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The organism facet covers aspects of organisms
			    description. The description ranges from life forms
			    the size of the organisms and a taxonomical
			    classification.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>

			    <xs:element name="organismicCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The organismic coverage deals with aspects of taxonomically classified
					organisms as well as with the characterization of life forms.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="organismRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The organismic range allows to specify multiple taxonomically
						    classified organisms.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="organismItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The organism item allows to capture a specific organisms by taxonomical
								classification.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="taxonomicClassification" type="TaxonomicClassification" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    The taxonomic rank name specifies the level of the taxonomic naming given
									    in the taxonomicName field in the organism item. This can be e.g.
									    domain, kingdom, phylum, division, class, order, family, genus, species
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="organismName" type="OrganismName" minOccurs="0" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Atomised names of the taxon, with subtypes according to the
									    different Codes of Nomenclature which apply to scientific
									    names.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			    <xs:element name="lifeFormCoverage" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="lifeFormRange" minOccurs="1" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The life form range allows to specify multiple types of life forms
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="lifeFormItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The life form item allows to specify a specific life form
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="lifeFormClass" type="LifeFormClass" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    A broad general classification of the life form (animal, plant, bacteria, virus)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="lifeFormType" type="LifeFormType" minOccurs="0" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Allows further specification of the life form covering various aspects (hydrophyte,
									    homöotherm ...).
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="chemical" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The chemical facet covers all information about
			    chemical elements, compounds or ions measured or
			    involved in the dataset.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>

			    <xs:element name="chemicalCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The chemical coverage module deals with
					aspects of elements and compounds that
					have been involved in the study and
					their classification.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="elementRange" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The chemical element range allows to specify elements
						    that have been involved in the study.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="elementItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The chemical element item allows to specify an element
								that has been involved in the study.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="elementName" type="ElementName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Specify a chemical element by name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

					<xs:element name="isotopeRange" minOccurs="0" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="isotopeItem" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="isotopeName" type="IsotopeName" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Specify an isotope by name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

					<xs:element name="compoundRange" minOccurs="0" maxOccurs="1">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The compound range allows to capture a range of chemical compound items
						    by their name and classification.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="compoundItem" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The compound item allows to caputre a specific chemical compund
								by name and classification.
							    </xs:documentation>
							</xs:annotation>
							<xs:complexType>
							    <xs:sequence>

								<xs:element name="compoundName" type="xs:string" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Caputures the compound name.
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="compoundClass" type="CompoundClass" minOccurs="1" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Caputres the compound class (organic, inorganic)
									</xs:documentation>
								    </xs:annotation>
								</xs:element>

								<xs:element name="compoundCategoryRange" minOccurs="0" maxOccurs="1">
								    <xs:annotation>
									<xs:documentation xml:lang="en">
									    Allows to capture a range of categories the compound belong to
									</xs:documentation>
								    </xs:annotation>
								    <xs:complexType>
									<xs:sequence>

									    <xs:element name="compoundCategoryItem" minOccurs="1" maxOccurs="unbounded">
										<xs:annotation>
										    <xs:documentation xml:lang="en">
											The compound category item captures one specific compound categorization.
										    </xs:documentation>
										</xs:annotation>
										<xs:complexType>
										    <xs:sequence>

											<xs:element name="compoundCategoryName" type="CompoundCategory" minOccurs="1" maxOccurs="1">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    The compound category captures one compound categorization by name (e.g. nucleobase,
												    nucleotide, arene, alkene, alcohol)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>

										    </xs:sequence>
										</xs:complexType>
									    </xs:element>

									</xs:sequence>
								    </xs:complexType>
								</xs:element>

							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			    <xs:element name="chemicalFunctionRange" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The chemical function range allows to capture a range of chemical functions
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="chemicalFunctionItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The chemical function item allows to capture one chemical function
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="chemicalFunctionName" type="chemicalFunctionNameType" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The chemical function name item allows to capture one chemical function (Antibody, Cofactor,
								Enzyme)
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			</xs:sequence>
		    </xs:complexType>
		</xs:element>

		<xs:element name="process" minOccurs="1" maxOccurs="1">
		    <xs:annotation>
			<xs:documentation xml:lang="en">
			    The process facet deals with aspects about
			    processes observed in the study. It covers the name
			    of the process and allows a classification of the
			    process with process types.
			</xs:documentation>
		    </xs:annotation>
		    <xs:complexType>
			<xs:sequence>

			    <xs:element name="processCoverage" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The process coverage deals with
					capturing processes and their
					classification.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="processItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The process item captures a
						    specific process by name
						    and its classifiction.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="processName" type="ProcessName" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The process name fields covers the process name.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						    <xs:element name="processClass" type="ProcessClass" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The process class covers the general character of the process. It
								can be interaction, transformation or transport.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						    <xs:element name="processType" type="ProcessType" minOccurs="0" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								The process type covers a more detailed classification of the process.
								It goes hand in hand with the process class.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			    <xs:element name="interactionRange" minOccurs="0" maxOccurs="1">
				<xs:complexType>
				    <xs:sequence>

					<xs:element name="interactionItem" minOccurs="1" maxOccurs="unbounded">
					    <xs:annotation>
						<xs:documentation xml:lang="en">
						    The interaction item captures an interaction by
						    the two interaction partners and a direction of
						    the interaction. Complex interactions that involve
						    more than one partner have to be specified split
						    into multiple parts.
						</xs:documentation>
					    </xs:annotation>
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="firstInteractionPartner" type="InteractionPartner" minOccurs="1" maxOccurs="1">
							<xs:annotation xml:lang="en">
							    <xs:documentation>
								The first partner in the interaction.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						    <xs:element name="directionOfInteraction" type="DirectionOfInteraction" minOccurs="1" maxOccurs="1">
							<xs:annotation xml:lang="en">
							    <xs:documentation>
								The direction of the interaction. This can be "Acts On", "Affected By", "Mutally Interact"
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						    <xs:element name="secondInteractionPartner" type="InteractionPartner" minOccurs="1" maxOccurs="1">
							<xs:annotation xml:lang="en">
							    <xs:documentation>
								The second partner in the interaction.
							    </xs:documentation>
							</xs:annotation>
						    </xs:element>

						</xs:sequence>
					    </xs:complexType>
					</xs:element>

				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			</xs:sequence>
		    </xs:complexType>
		</xs:element>

	    </xs:sequence>
	</xs:complexType>
    </xs:element>
	</xs:sequence>
	</xs:complexType>
    </xs:element>

    <!-- simple types -->
    <xs:simpleType name="NonNegativeDecimal">
	<xs:restriction base="xs:decimal">
	    <xs:minExclusive value="0"/>
	</xs:restriction>
    </xs:simpleType>

    <!-- complex types -->

    <xs:complexType name="TaxonomicClassification">
	<!-- cite eml  -->
	<xs:sequence>

	    <xs:element name="taxonRankName" type="xs:string" minOccurs="1">
	    </xs:element>

	    <xs:element name="taxonRankValue" type="xs:string" minOccurs="1">
	    </xs:element>

	    <xs:element name="commonName" type="xs:string" minOccurs="0" maxOccurs="unbounded">
	    </xs:element>

	    <xs:element name="taxonomicClassification" type="TaxonomicClassification" minOccurs="0" maxOccurs="1">
	    </xs:element>

	</xs:sequence>
    </xs:complexType>

</xs:schema>
