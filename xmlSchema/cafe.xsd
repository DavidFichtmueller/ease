<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" version="0.1" >
  <xs:element name="Cafe">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	The Cafe element is the root element of the Comprehensive Annotation
	for Ecology schema. It designates the start of the annotation and
	captures all information describing a search object.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
	<xs:element name="Object">
	  <xs:annotation>
	    <xs:documentation xml:lang="en">
	      The Object field describes one cafe object it contains all the
	      context information that is provided for a search object with
	      cafe.
	    </xs:documentation>
	  </xs:annotation>
	  <xs:complexType>
	    <xs:sequence>
	      <xs:element name="Contexts">
		<xs:annotation>
		  <xs:documentation xml:lang="en">
		    The Contexts field is a wrapper around all different
		    contexts that describe the search object in detail.
		  </xs:documentation>
		</xs:annotation>
		<xs:complexType>
		  <xs:sequence>
		    <xs:element name="Context">
		      <xs:annotation>
			<xs:documentation xml:lang="en">
			  The Context element holds a single context
			</xs:documentation>
		      </xs:annotation>
		      <xs:complexType>
			<xs:sequence>
			  <xs:element name="PhysicalContext">
			    <xs:annotation>
			      <xs:documentation xml:lang="en">
				All object references are captured under this element.
				These cover for example ids in databases names of files
				and types of the data as well as the data center that
				hosts the data
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="ObjectIds">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The object ids is a wrapper that
				      contains all sorts of ids that are
				      involved linking the annotation with
				      the actual search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ObjectId">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The object id is a container holds one object
					    id of all potentially possible object ids. This
					    element exists for extendability of the schema and
					    provides a semantic anchor.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:integer" name="databaseId">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The database id is an integer that proides a link
						  between the data and the annotation. This field can
						  be used system internally.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="ObjectNames">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The object names is a wrapper that
				      contains all sorts of names that are
				      involved naming the object of annotation.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ObjectName">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The object name is a container holds one name of
					    all potentially possible names of described objects.
					    This element exists for extendability of the schema
					    and provides a semantic anchor.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:string" name="fileName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The file name provides a link to the actual search object
						  as it is the name of the raw data file that we can download
						  including its mime type. Fore example 19.11.1996_plot11.jpeg
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>

				<xs:element name="ObjectDescriptions">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper that contains all sorts of
				      object descriptions
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ObjectDescription">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    An object description can hold an object description
					    and a title.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="generalAbsract" type="xs:string">
					      <xs:annotation xml:lang="en">
					        <xs:documentation>
						  Abstract that describes the object in all
						  its details. This can contain any information
						  that is suitable to inform somebody who is going
						  to search for data. Write in great detail about
						  all aspects that characterize the data (e.g. setup)
					        </xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element name="objectTitle" type="xs:string">
					      <xs:annotation xml:lang="en">
					        <xs:documentation>
					          A title of the search object.
					        </xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="ObjectTypes">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The object types is a wrapper that
				      contains all sorts of types that are
				      needed to describe the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ObjectType">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The object type is a container holds one tpye of
					    all potentially possible types associated with an object.
					    This element exists for extendability of the schema and
					    provides a semantic anchor.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="FileType" name="fileType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The file type provides a link to the actual search object
						  as it is the type of the raw data file that we can download.
						  For example (image, table, audio, video)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="ObjectUris">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The object uris is a wrapper that
				      contains all sorts of uris that are
				      needed to reference the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ObjectUri">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The object uri is a container that holds one uri type of
					    all potentially possible uri types associated with an object.
					    This element exists for extendability of the schema and provides
					    a semantic anchor.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:choice>
					      <xs:element type="xs:anyURI" name="objectDownloadUrl">
						<xs:annotation>
						  <xs:documentation xml:lang="en">
						    The URL to the download of the data. Using this URL with
						    a download manager should return the raw data e.g. csv or
						    whatever kind of data type is described.
						  </xs:documentation>
						</xs:annotation>
					      </xs:element>
					      <xs:element type="xs:anyURI" name="objectReferenceUrl">
						<xs:annotation xml:lang="en">
						  <xs:documentation>
						    The object reference URL can be used to link the annotation
						    to more contextual information for example the project homepage
						    with more extensive metadata of the search object described with
						    the annotation.
						  </xs:documentation>
						</xs:annotation>
					      </xs:element>
					    </xs:choice>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="ObjectLocations">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The object locations is a wrapper that
				      contains all sorts of location that are
				      needed to reference the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ObjectLocation">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The object location is a
					    container that holds one
					    object location type of all
					    potentially possible object
					    location types. This
					    element exists for
					    extendability of the schema
					    and provides a semantic
					    anchor.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:string" name="dataCenter">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The data center where the downloadable data is hosted at. For
						  example: Data Center Senckenberg (SGN), Botanic Garden and
						  Botanical Museum Berlin-Dahlem, Data Publisher for Earth and
						  Environmental Science (PANGAEA).
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="TimeContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The time context allows to capture the
				quantitative and qualitative temporal aspects
				that describe the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="TimeRanges">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      An element that can hold multiple time ranges
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="TimeRange">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    The time range captures the range of times and dates
					    relevant for the search object. This can be e.g. the start
					    and end date and time of the data aquisition. If both the
					    start and the end date and time are equal this represents
					    a point in time that would be suitable for example describing
					    the point in time an image was taken.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="LocalTimeRange">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  This element captures a local format time range with
						  date, time and olson timezone
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element name="RangeStart">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The start of the time range in form of a local date and local
							time associated with a timezone. This can be e.g. the start of
							the data aquisition.
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="localDate">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A single date in form YYYY-MM-DD conform to iso 8601.
							    </xs:documentation>
							  </xs:annotation>
							</xs:element>
							<xs:element type="xs:time" name="localtime">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A single time in form HH:MM:SS conform to iso 8601.
							    </xs:documentation>
							  </xs:annotation>
							</xs:element>
							<xs:element type="OlsonTimezone" name="olsonTimezone">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      The olson timezone to capture the timezone of the
							      given date and time in the range (e.g. Europe/Berlin)
							    </xs:documentation>
							  </xs:annotation>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						  <xs:element name="RangeEnd">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The end of the time range in form of a local date and local
							time associated with a timezone. This can be e.g. the end of
							the data aquisition.
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element type="xs:date" name="localDate">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A single date in form YYYY-MM-DD conform to iso 8601.
							    </xs:documentation>
							  </xs:annotation>
							</xs:element>
							<xs:element type="xs:time" name="localtime">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A single time in form HH:MM:SS conform to iso 8601.
							    </xs:documentation>
							  </xs:annotation>
							</xs:element>
							<xs:element type="OlsonTimezone" name="olsonTimezone">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      The olson timezone to capture the timezone of the
							      given date and time in the range (e.g. Europe/Berlin)
							    </xs:documentation>
							  </xs:annotation>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="TimePeriods">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The time periods is a wrapper that
				      can contain all sorts of time periods that reference
				      the temporal context of the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="TimePeriod">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    The time period contains one possible time period
					    describing the search object.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="geologicalTimePeriod">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The geological age is a container that holds one
						  of the possible geological ages which is relevant for
						  describing the search object. E.g. a sample that comes
						  from the ordovician time period.
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element type="xs:string" name="geologicalEon">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The geological eon contextualizing the search object. For
							example: Phanerozoic, Precambrian.
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="geologicalEra">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The geological era contextualizing the search object. For
							example: Cenozoic, Mesozoic, Paleozoic.
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="geologicalPeriod">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The geological period contextualizing the search object. For
							example: Quaternary, Neogene, Paleogene.
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="geologicalEpoch">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The geological epoch contextualizing the search object. For
							example: Holocene, Pleistocene, Pilocene.
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="geologicalAge">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The geological age contextualizing the search object. For
							example: Calabrian, Gelasian, Piacenzian.
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="TemporalResolutions">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around temporal aspects describing
				      the temporal resolution of the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="TemporalResolution">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    The temporal resolution caputres
					    the temporal aspects of the setup
					    or study the search object comes
					    from.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="TemporalExtentType" name="temporalExtentType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The temporal extent type is a categorical value that describes
						  the extent of the setup the search object comes from (e.g.
						  Second, Minute, Hour, Day, Week, ...).
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="TemporalResolutionType" name="temporalResolutionType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The temporal resolution is a categorical value that describes
						  the resolution of the setup the search object comes from (e.g.
						  Second, Minute, Hour, Day, Week, ...).
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="SpaceContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The space context allows to capture the
				quantitative and qualitative spatial aspects
				that describe the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="BoundingBoxes">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      A wrapper that can hold several bounding boxes.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="BoundingBox">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The bounding box captures the spatial extent by four geographic
					    coordinates (N,E,S,W) in decimal degree.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="NorthBoundingCoordinate" name="northBoundingCoordinate">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The north most geographic coordinate that signifies the boundary of the data. It
						  has to be given in decimal degree between -90 and +90 degree.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="SouthBoundingCoordinate" name="southBoundingCoordinate">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The soutch most geographic coordinate that signifies the boundary of the data. It
						  has to be given in decimal degree between -90 and +90 degree.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="EastBoundingCoordinate" name="eastBoundingCoordinate">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The east most geographic coordinate that signifies the boundary of the data. It
						  has to be given in decimal degree between -180 and +180 degree.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="WestBoundingCoordinate" name="westBoundingCoordinate">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The west most geographic coordinate that signifies the boundary of the data. It
						  has to be given in decimal degree between -180 and +180 degree.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Elevations">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wraper that can hold several elevations.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Elevation">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    The elevation complements the
					    bounding box and specifies the
					    upper and lower most boundaries in
					    space contextualilzing the search
					    object.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:decimal" name="maximumElevation">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The maximum elevation contextualizing the search object
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ElevationUnit" name="maximumElevationUnit">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The unit the maximum elevation is given in (e.g. Meters)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="xs:decimal" name="minimumElevation">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The minimum elevation contextualizing the search object
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ElevationUnit" name="minimumElevationUnit">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The unit the minimum elevation is given in (e.g. Meters)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ElevationDatum" name="elevationDatum">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The reference point for the given elevation
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Coordinates">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      The coordinates element is a wrapper that
				      can capture all sorts of coordinates that are
				      relevant for the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Coordinate">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    Holds one single coordinate
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="UtmCoordinate">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The utm coordinate is a container that holds one
						  utm coordinate relevant for the search object.
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element type="xs:integer" name="utmCoordinateZone">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The numerical zone
							corresponding to the
							central meridian
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:integer" name="utmCoordinateSubZone">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The subzone letter
							corresponding to one of
							the 20 north-south
							divisions of the UTM
							grid system
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="UtmCoordinateHemisphere" name="utmCoordinateHemisphere">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The hemisphere. This can be N (North) or S (South)
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:decimal" name="utmCoordinateEasting">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The distance east of the origin of the UTM zone
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="UtmCoordinateUnit" name="utmCoordinateEastingUnit">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The unit of the distance of the easting (Meter, Kilometer)
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:decimal" name="utmCoordinateNorthing">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The distance north of the origin of the UTM zone
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="UtmCoordinateUnit" name="utmCoordinateNorthingUnit">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The unit of the distance of the northing (Meter, Kilometer)
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="UtmCoordinateGeodeticDatum" name="utmCoordinateGeodeticDatum">
						    <xs:annotation>
						      <xs:documentation xml:lang="en">
							The datum (e.g. wgs84)
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="NamedLocations">
				  <xs:annotation>
				    <xs:documentation xml:lang="en">
				      Named locations is a wrapper that
				      contains all sorts of named locations
				      contextualizing the search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="NamedLocation">
					<xs:annotation>
					  <xs:documentation xml:lang="en">
					    The named location is a container holds one named
					    location. This element exists for extendability of
					    the schema and provides a semantic anchor.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:string" name="locationName">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The name of the location (e.g. Leipzig, Cospudener See, Mount Everest)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="LocationType" name="locationType">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  The type of the location. This could be e.g. City, Lake, Study Site etc.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="CountryName" name="countryName">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  Name of the country of the given location name.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ContinentName" name="continentName">
					      <xs:annotation>
						<xs:documentation xml:lang="en">
						  Name of the continent of the given location.
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="SpatialResolutions">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around spatial aspects describing
				      the spatial resolution contextualizing the
				      search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="SpatialResolution">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    The spatial resolution caputres
					    the spatial aspects of the setup
					    or study the search object comes
					    from.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="SpatialResolutionType" name="spatialExtentType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The spatial resolution type
						  is a categorical value that
						  describes the spatial
						  resolution of the setup the
						  search object comes from
						  (e.g. Point, Plot, Region,
						  Continent, Global).
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="SpatialResolutionType" name="spatialResolutionType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The spatial extent type
						  is a categorical value that
						  describes the spatial
						  extent of the setup the
						  search object comes from
						  (e.g. Point, Plot, Region,
						  Continent, Global).
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="SphereContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The sphere context allows to capture the
				quantitative and qualitative sphere related aspects
				that describe the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="Spheres">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      Spheres is a wrapper that holds all sorts of
				      information about spheres that describe the
				      search object.
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Sphere">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    Sphere contains information about a single
					    sphere.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="Atmosphere">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The atmosphere element is a wrapper
						  around all qualitative and  quantitative
						  values of the atmosphere.
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element name="NamedAtmosphereLayers">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							Named atmosphre layers is a
							wrapper around all sorts of
							named atmosphere layers.
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="NamedAtmosphereLayer">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      Captures the name of a
							      single atmosphere layer
							      that describing the
							      search object. E.g.
							      Troposphere when air
							      temperature measures
							      have been taken. (also.
							      Strato-, Meso-,
							      Ionosphere)
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element type="AtmosphereLayerName" name="atmosphereLayerName">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The name of an atmopshere layer. This can be for
								    example strato, tropo, meso, ionosphere
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						  <xs:element name="NumericAtmosphereLayers">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							Numeric atmosphere layers is
							a wrapper around all sorts of
							numeric aspects of
							atmospheric layers.
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="NumericAtmosphereLayer">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      Numeric atmosphere
							      layer allows to specify
							      an atmospheric layer
							      relevant for the data
							      in a numeric form.
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element type="xs:decimal" name="minimumAtmosphereHeight">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The minimum height in the atmosphere relevant for the search object.
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element type="AtmosphereHeightUnit" name="minimumAtmosphereHeightUnit">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The unit the minimum height in atmosphere is given in (Meters, Kilometers).
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element type="xs:decimal" name="maximumAtmosphereHeight">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The maximum height in the atmosphere relevant for the search object.
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element type="AtmosphereHeightUnit" name="maximumAtmosphereHeightUnit">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The unit the maximum height in atmosphere is given in (Meters, Kilometers).
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					    <xs:element name="Hydrosphere">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The hydrosphere element is a wrapper
						  around all qualitative and quantitative
						  values revolving around the hydrosphere
						  the contextualize the search object.
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element name="HydrosphereCompartments">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The hydrosphere compartments captures
							all compartments that are described regarding
							the hydrosphere (River, Lake, Sea)
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="HydrosphereCompartment">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      The hydrosphere compartment documents one
							      hydrosphere compartment
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="River">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The river element is a wrapper
								    around all qualitative and quantitative
								    values of a river.
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element name="NamedRiverZones">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  The river zones is a wrapper
									  around all qualitative and
									  quantitative values of the
									  zones of a river.
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="NamedRiverZone">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										The river zone contains one possible river zone.
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="LongitudinalRiverZone" name="longitudinalRiverZone">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The longitudinal river zone contains one
										      vertical river zone describing the search object
										      (Rhitron, Crenon or Potamon)
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="VerticalRiverZone" name="verticalRiverZone">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The vertical river zone contains one
										      vertical river zone describing the search object
										      (Floodplain, River bank, River Water, River Bed)
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							      <xs:element name="Lake">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The lake element is a wrapper
								    around all qualitative and quantitative
								    values of a lake.
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element name="NamedLakeZones">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  The lake zones is a wrapper
									  around all qualitative and
									  quantitative values of the
									  zones of a lake.
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="NamedLakeZone">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										A lake zone contains
										one possible lake zone
										describing the search
										object.
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="BenthicLakeZone" name="benthicLakeZone">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      Benthic lake zones contains
										      one benthic zone describing the
										      search object (lithoral,
										      benthic, profundal).
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="PelagicLakeZone" name="pelagicLakeZone">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      Pelagig lake zone contains one
										      pelagig zone describing the
										      search object (epilimnion,
										      metalimnion, hypolimnion).
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							      <xs:element name="Sea">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The sea element is a wrapper
								    around all qualitative and quantitative
								    values of the sea.
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element name="NamedSeaZones">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  The sea zones is a wrapper
									  around all qualitative and
									  quantitative values of the
									  zones of a sea.
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="NamedSeaZone">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										The sea zones
										contains one possible
										sea zone.
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="BenthicSeaZone" name="benthicSeaZone">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      A benthic sea zone (litoral, neritic, bathyal, abyssal, hadal)
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="PelagicSeaZone" name="pelagicSeaZone">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      A pelagic sea zone (epipelagic, mesopelagic, bathypelagic).
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					    <xs:element name="Pedosphere">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The pedosphere element is a wrapper
						  around all qualitative and quantitative
						  values of the pedosphere.
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element name="PedosphereCompartments">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The pedosphere compartements captures
							all compartements that are described regarding
							the pedosphere (Soil)
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="PedosphereCompartment">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      The pedosphere compartment documents one
							      pedosphere compartment
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="Soil">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    The soil element captures all qualitative
								    and quantitative characteristics regarding
								    the pedosphere compartement soil
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element name="NamedSoilLayers">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  The soil layers element is a
									  wrapper around all qualitative and
									  quantitative values of the
									  named pedosphere layers.
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="NamedSoilLayer">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										Captures one named soil layer
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="SoilHorizon" name="soilHorizon">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The name of a soil horizon. O, A, B, C, R horizon
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								    <xs:element name="NumericSoilLayers">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  The numeric soil layers is an element
									  to capture layers of the soil numerically
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="NumericSoilLayer">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										Captures a numeric soil layer
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="xs:decimal" name="minimumSoilDepth">
										  <xs:annotation>
										    <xs:documentation xml:lang="en">
										      A decimal number of the minimum depth of the layer.
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="SoilDepthUnit" name="minimumSoilDepthUnit">
										  <xs:annotation>
										    <xs:documentation xml:lang="en">
										      The unit the minimum depth of the layer is given in (Meters, Centimeters).
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="xs:decimal" name="maximumSoilDepth">
										  <xs:annotation>
										    <xs:documentation xml:lang="en">
										      A decimal number of the maximum depth of the layer.
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="SoilDepthUnit" name="maximumSoilDepthUnit">
										  <xs:annotation>
										    <xs:documentation xml:lang="en">
										      The unit the maximum depth of the layer is given in (Meters, Centimeters).
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								    <xs:element name="SoilTextures">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  Caputures all qualitative and quantitative characteristics
									  regarding soil texture.
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="SoilTexture">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										The soil texture captures a single soil texture
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="xs:integer" name="sandPercent">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The percentage of sand present in the soil
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="xs:integer" name="siltPercent">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The percentage of silt present in the soil
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
										<xs:element type="xs:integer" name="loamPercent">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The percentage of loam present in the soil
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								    <xs:element name="SoilAcidities">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  Caputres all qualitative and quantitative characteristics
									  regarding soil acidity
									</xs:documentation>
								      </xs:annotation>
								      <xs:complexType>
									<xs:sequence>
									  <xs:element name="SoilAcidity">
									    <xs:annotation xml:lang="en">
									      <xs:documentation>
										The soil acidity captures one soil acidity
									      </xs:documentation>
									    </xs:annotation>
									    <xs:complexType>
									      <xs:sequence>
										<xs:element type="SoilAcidity" name="soilAcidity">
										  <xs:annotation xml:lang="en">
										    <xs:documentation>
										      The soil acidity is a categorical field that captures the
										      acidity of the soil (Acidic, Neutral, Basic)
										    </xs:documentation>
										  </xs:annotation>
										</xs:element>
									      </xs:sequence>
									    </xs:complexType>
									  </xs:element>
									</xs:sequence>
								      </xs:complexType>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					    <xs:element name="Ecosphere">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The ecosphere element is a wrapper
						  around all qualitative and quantitative
						  values of the ecosphere.
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element name="NamedEcosphereLayers">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							A wrapper around named ecosphere layers
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="NamedEcosphereLayer">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A named ecosphere layer
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element type="EcosphereLayerName" name="ecosphereLayerName">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    A named layer in the ecosphere (Tree- , Shrub-, Herb-, Moss-, Ground Layer)
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						  <xs:element name="NumericEcosphereLayers">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							A wrapper around numeric ecosphere layers
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="NumericEcosphereLayer">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A numeric ecosphere layer
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element type="xs:decimal" name="minimumVegetationHeight">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    A decimal number of the minimum depth of the vegetation height.
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element type="VegetationHeightUnit" name="minimumVegetationHeightUnit">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The unit the minimum height of the layer is given in (e.g. Meters, Centimeters).
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element type="xs:decimal" name="maximumVegetationHeight">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    A decimal number of the maximum depth of the vegetation height.
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element type="VegetationHeightUnit" name="maximumVegetationHeightUnit">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The unit the maximum height of the layer is given in (e.g. Meters, Centimeters).
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						  <xs:element name="OrganizationalHierarchies">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							A wrapper around organizational hierarchies
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="OrganizationalHierarchy">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A organizational hierarchy
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element type="OrganizationHierarchyName" name="organizationHierarchyName">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    A named organizational hierarchy of life (Atom, Molecule, Cell, Cell Organelle, ...)
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="BiomeContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The biome context allows to capture the
				quantitative and qualitative biome related aspects
				that describe the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="ZonoBiomes">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around zono biomes
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="ZonoBiome">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A zono biome
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="BiomeType" name="biomeType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The biome type (Terrestrial, Freshwater, Marine)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="BiomeZoneName" name="biomeZoneName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The name of a biome zone (Polar, Boreal, Temperate, Subtropic, Tropic)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="BiomeWaterAvailability" name="biomeWaterAvailability">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The water availability within the biome (Semi Humid, Humid, Arid, Semi Arid)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="BiomeInfluence" name="biomeInfluence">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The influence to the biome (Continental, Maritime)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="BiomeHemisphere" name="biomeHemisphere">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The hemisphere the biome is located in (North, South)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="OroBiomes">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around oro biomes
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="OroBiome">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    An oro biome.
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="OroBiomeType" name="oroBiomeType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The type of oro biome (Nivale, Subalpine, Alpine, Montane, Submontane, Planar)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="PedoBiomes">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around pedo biomes
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="PedoBiome">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A pedo biome
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="PedoBiomeType" name="pedoBiomeType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of pedo biome (Amphibiome, Halobiome, Helobiome, Hydrobiome, Lithobiome, Peinobiome, Psammobiome)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Physiognomies">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around physiognomies
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Physiognomy">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A physiognomy
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="PhysiognomyTypes">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A wrapper around physiognomy types
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element name="PhysiognomyType">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							A physiognomy type
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="TerrestrialPhysiognomies">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A wrapper around terrestrial physiognomies
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="TerrestrialPhysiognomy">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    A terrestrial physiognomy
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element type="TerrestrialPhysiognomyType" name="terrestrialPhysiognomyType">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  A type of terrestrial physiognomy (Evergreen Needleleaf Forest, Evergreen
									  Broadleaf Forest, Deciduous Needleleaf Forest, Deciduous Broadleaf Forest)
									</xs:documentation>
								      </xs:annotation>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
							<xs:element name="SemiAquaticPhysiognomies">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A wrapper around semi aquatic physiognomies
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="SemiAquaticPhysiognomy">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    A semi aquatic physiognomy
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element type="SemiAquaticPhysiognomyType" name="semiAquaticPhysiognomyType">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  A type of semi aquatic physiognomy (Mire, Bog, Fan, Saltmarsh)
									</xs:documentation>
								      </xs:annotation>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
							<xs:element name="AquaticPhysiognomies">
							  <xs:annotation xml:lang="en">
							    <xs:documentation>
							      A wrapper around aquatic physiognomies
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="AquaticPhysiognomy">
								<xs:annotation xml:lang="en">
								  <xs:documentation>
								    An aquatic physiognomy
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element type="PlantCharacterizedAquaticPhysiognomyType" name="plantCharacterizedAquaticPhysiognomyType">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  A plant characterized aquatic physiognomy type (Emergent Aquatic, Floating Leave Aquatic ...)
									</xs:documentation>
								      </xs:annotation>
								    </xs:element>
								    <xs:element type="HabitatCharacterizedAquaticPhysiognomy" name="habitatCharacterizedAquaticPhysiognomy">
								      <xs:annotation xml:lang="en">
									<xs:documentation>
									  A habitat characterized aquatic physiognomy type (Estuary, Kelp Forest, Coral Reef, Cold Seep, ...)
									</xs:documentation>
								      </xs:annotation>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="LandUses">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around land uses
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="LandUse">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A land use
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="LandUseType" name="landUseType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of land use (Natural, Semi Natural, Urban)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="LandUseForm" name="landUseForm">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of land use form (Agriculture, Agroforestry, Forestry, Aquaculture)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="OrganismContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The organism context captures the quantitative and
				qualitative organism related aspects that describe
				the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="Organisms">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around organisms
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Organism">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    An organism
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element name="Taxonomy">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The taxonomy of the organism
						</xs:documentation>
					      </xs:annotation>
					      <xs:complexType>
						<xs:sequence>
						  <xs:element type="xs:string" name="domain">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The domain of the organism
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="kingdom">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The kingdom of the organism
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="division">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The division of the organism
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="class">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The class of the organism
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="order">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The order of the organism
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element type="xs:string" name="family">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The family of the organism
						      </xs:documentation>
						    </xs:annotation>
						  </xs:element>
						  <xs:element name="organismName">
						    <xs:annotation xml:lang="en">
						      <xs:documentation>
							The name of an organism
						      </xs:documentation>
						    </xs:annotation>
						    <xs:complexType>
						      <xs:sequence>
							<xs:element name="bacterialName" >
							  <xs:annotation>
							    <xs:documentation xml:lang="en">
							      Atomised name with elements according to the
							      International Code of Nomenclature of Prokaryotes (ABCD)
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="genusOrMonomial" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Genus name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="subgenus" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Subgenus name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="subgenusAuthorAndYear" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Author team and year of the subgenus name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="speciesEpithet" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    A name element dependent on a genus or a species name
								    under the bacteriological, botanical, and zoological
								    code.
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="subspeciesEpithet" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The subsphecies epithet (Maps to Darwin Core 2: Subspecies)
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="parentheticalAuthorTeamAndYear" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Author team and year of the basionym of a species or
								    subspecies combination
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="authorTeamAndYear" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The author(s) who published the full name as it stands
								    and its year
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="nameApprobation" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Approbation of name according to approved list,
								    validation list,or validly published, paper in IJSB
								    after 1980
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
							<xs:element name="botanicalName">
							  <xs:annotation>
							    <xs:documentation xml:lang="en">
							      Atomised name with elements according to the
							      International Code of Botanical Nomenclature
							      or the International Code of Nomenclature for
							      Cultivated Plants (ABCD)
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="genusOrMonomial" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Genus or higher taxon name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="firstEpithet" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The first epithet in a botanical name combination
								    (species epithet or the epithet of the subdivision of a
								    genus
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="infraspecificEpithet" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The final epithet of a botanical name of infraspecific
								    rank
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="rank" type="RankAbbreviation" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The rank of the name where needed to form it.
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="hybridFlag" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Flag indicating that this is a named hybrid ("x") or a
								    graft chimaera ("+"). The attribute insertionpoint
								    defines the name element in front of which the
								    qualifier should be inserted (1 = before entire name,
								    2= before first epithet, 3 = before rank designation
								    for second epithet.)
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:simpleContent>
								    <xs:extension base="xs:string">
								      <xs:attribute name="insertionpoint" type="Insertpoint"/>
								    </xs:extension>
								  </xs:simpleContent>
								</xs:complexType>
							      </xs:element>
							      <xs:element name="authorTeamParenthesis" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Author team of the basionym of a combination
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="authorTeam" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The author(s) who published the full name as it stands
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="cultivarGroupName" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Cultivar group name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="cultivarName" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Cultivar name according to the ICNCP
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="tradeDesignationNames" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Container for trade names
								  </xs:documentation>
								</xs:annotation>
								<xs:complexType>
								  <xs:sequence>
								    <xs:element name="tradeDesignationName" type="xs:string" minOccurs="0" maxOccurs="unbounded">
								      <xs:annotation>
									<xs:documentation xml:lang="en">
									  Trade name used for a specific cultivar
									</xs:documentation>
								      </xs:annotation>
								    </xs:element>
								  </xs:sequence>
								</xs:complexType>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
							<xs:element name="zoologicalName">
							  <xs:annotation>
							    <xs:documentation xml:lang="en">
							      Atomised name with elements according to the
							      International Code of Zoological Nomenclature (ABCD)
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="genusOrMonomial" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Genus or higher taxon name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="subgenus" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Subgenus name
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="speciesEpithet" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Species epithet
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="subspeciesEpithet" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Maps to Darwin Core 2: Subspecies
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="authorTeamOriginalAndYear" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The first person(s) who validly published a
								    species-group name and the year of publication
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="authorTeamParenthesisAndYear" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The parenthetical citation of the original author when a species-group was
								    transferred to another genus and the year of the original
								    publication
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="combinationAuthorTeamAndYear" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The citation of the authors responsible for the new combination and the year of
								    its publication
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="breed" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Name of the breed of an animal
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="namedIndividual" type="xs:string" minOccurs="0"/>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
							<xs:element name="viralName">
							  <xs:annotation>
							    <xs:documentation xml:lang="en">
							      Atomised name with elements according to the
							      International Committee on Taxonomy of Viruses
							    </xs:documentation>
							  </xs:annotation>
							  <xs:complexType>
							    <xs:sequence>
							      <xs:element name="genusOrMonomial" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    Genus or higher taxon name formed according to the code
								    and representing the result of the identification. E.g.
								    Comovirus
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="viralSpeciesDesignation" type="xs:string" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The formal name of a viral species. Examples: vaccinia
								    virus; human herpes virus 2 (herpes simplex virus 2);
								    poliovirus 1; Saccharomyces cerevisiae virus L-A;
								    enterobacteria phage PRD1 [Source: ICTV]
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							      <xs:element name="acronym" minOccurs="0">
								<xs:annotation>
								  <xs:documentation xml:lang="en">
								    The accepted acronym for the Virus, e.g. PCV for Peanut Clump Virus
								  </xs:documentation>
								</xs:annotation>
							      </xs:element>
							    </xs:sequence>
							  </xs:complexType>
							</xs:element>
						      </xs:sequence>
						    </xs:complexType>
						  </xs:element>
						</xs:sequence>
					      </xs:complexType>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="ProcessContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The process context captures the quantitative and
				qualitative process related aspects that describe
				the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="Processes">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around processes
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Process">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A process
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="ProcessName" name="processName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A named process  (Adaption, Speciation, Migration, Birth, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ProcessFocus" name="processFocus">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The focus of the process given under Process Name for classification
						  (Organism, Chemical, Matter, Energy)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ProcessType" name="processType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The process type of the process given under Process Name for classification
						  (Motion, Uptake, Release, Exchange)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Interactions">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around interactions
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Interaction">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    An interaction
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:string" name="interactionName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A named interaction (Predation, Parasisitm, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="InteractionPartner" name="interactionPartnerOne">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of interaction partner (Plant, Animal, Fungi, Bacteria,
						  Archaea, Virus, Environment)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="InteractionDirection" name="interactionDirection">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A direction of the interaction (Acts On, Is Affected By, Mutually Interacts With)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="InteractionQuality" name="interactionQuality">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  An interaction quality (Neutralism, Amensalism, Commensalism, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="InteractionPartner" name="interactionPartnerTwo">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of interaction partner (Plant, Animal, Fungi, Bacteria,
						  Archaea, Virus, Environment)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="ChemicalContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The chemical context allows to capture the
				quantitative and qualitative chemical related aspects
				that describe the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="Elements">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around chemical elements
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Element">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A chemical element
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="ElementName" name="elementName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A named chemical element
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="IsotopeName" name="isotopeName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A named chemical isotope
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Compounds">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around chemical compunds
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Compound">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A chemical compound
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="xs:string" name="compoundName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A name of a chemical compound
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="CompoundClass" name="compoundClass">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A compound class for the chemical compound given under
						  compund name as classification (Biomolecule, Aromatic,
						  Aliphatic, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="CompoundType" name="compoundType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A compound type for the chemical compound given under
						  compund name as classification (Nucleosides, Nucleotides,
						  Amino Acid, Protein, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Functions">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around chemical functions
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Function">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A chemical function
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="ChemicalFunctionType" name="chemicalFunctionType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A chemical function type (Repellent, Reservoir, Stimulator, Structure, Antibody, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			  <xs:element name="MethodContext">
			    <xs:annotation xml:lang="en">
			      <xs:documentation>
				The method context allows to capture the
				quantitative and qualitative method related aspects
				that describe the the search object.
			      </xs:documentation>
			    </xs:annotation>
			    <xs:complexType>
			      <xs:sequence>
				<xs:element name="Approaches">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around approaches
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Approach">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    An approach used by the study
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="ApproachType" name="ApproachType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of approach used (Virtual, Manipulative, Observational)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="ApproachLocalization" name="approachLocalization">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A type of approach localization (Microcosm, Mesocosm, Macrocosm)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
				<xs:element name="Factors">
				  <xs:annotation xml:lang="en">
				    <xs:documentation>
				      A wrapper around factors
				    </xs:documentation>
				  </xs:annotation>
				  <xs:complexType>
				    <xs:sequence>
				      <xs:element name="Factor">
					<xs:annotation xml:lang="en">
					  <xs:documentation>
					    A factor relevant for the study
					  </xs:documentation>
					</xs:annotation>
					<xs:complexType>
					  <xs:sequence>
					    <xs:element type="FactorName" name="factorName">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  A name of a factor used in the study or setup
						  (Producer Diversity, Consumer Density, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					    <xs:element type="FactorType" name="factorType">
					      <xs:annotation xml:lang="en">
						<xs:documentation>
						  The type of factor given with Factor Name (Biotic, Abiotic, Disturbance, ...)
						</xs:documentation>
					      </xs:annotation>
					    </xs:element>
					  </xs:sequence>
					</xs:complexType>
				      </xs:element>
				    </xs:sequence>
				  </xs:complexType>
				</xs:element>
			      </xs:sequence>
			    </xs:complexType>
			  </xs:element>
			</xs:sequence>
		      </xs:complexType>
		    </xs:element>
		  </xs:sequence>
		</xs:complexType>
	      </xs:element>
	    </xs:sequence>
	  </xs:complexType>
	</xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="FileType">
    <xs:annotation xml:lang="en">
      <xs:documentation>
	The file type provides information about the type of search object. It
	is an extendable list of object types (Image, Audio, Table, Video)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Audio">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Audio file tpye (e.g. mp3)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Image">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Image file tpye (e.g. jpeg)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Table">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Tabular file tpye (e.g. csv, excel)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Video">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Video file tpye (e.g. mp4, avi)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <!-- extension -->
      <xs:pattern value="[A-zäöüÄÖÜß ]*"/>
    </xs:restriction>
  </xs:simpleType>
