														    <xs:element name="maximumValue" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
														    <xs:element name="unit" type="LengthUnit" minOccurs="1" maxOccurs="1"/>
														</xs:sequence>
													    </xs:complexType>
													</xs:element>
												    </xs:sequence>
												</xs:complexType>
											    </xs:element>

											    <xs:element name="texture" minOccurs="0" maxOccurs="1">
