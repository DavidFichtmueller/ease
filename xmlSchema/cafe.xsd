									<!-- <xs:documentation xml:lang="en"> -->
									    <!-- The temporalNameItem captures one temporal name each. -->
									    <!-- </xs:documentation> -->
									<!-- </xs:annotation> -->
								    <!-- <xs:complexType> -->
									<!-- <xs:sequence> -->
									    <!-- <xs:element name="geologicalTimePeriod" type="GeologicalTimePeriod" minOccurs="1" maxOccurs="1"> -->
