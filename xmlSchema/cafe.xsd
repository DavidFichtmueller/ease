  <xs:simpleType name="FactorName">
    <xs:restriction base="xs:string">
      <!-- biotic -->
      <xs:enumeration value="Producer Diversity">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    This includes richness, functional diversity, shannon diversity
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Producer Composition">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The composition of producers. This also includes occurence.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Producer Density">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    All density or packing related manipulation of
	    consumers. Ammount of certain individuals per plot.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Producer Structure">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The size, height, leaf area index, age structure
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Consumer Diversity">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    All diversity related factor of consumers, richness,
	    shannon
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Consumer Composition">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Including network struture, occurence, exclusion
	    experiment.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Consumer Density">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    All spatial related manipulations to consumers (e.g.
	    density of grashoppers per plot)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Consumer Structure">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    All structure related factors of consumers, (e.g. body size
	    distribution age structure).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <!-- abiotic -->
      <xs:enumeration value="Radiation Intensity">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The intensity radiation for example photosynthetic
	    active radiation.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Radiation Quality">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The quality of the radiation. For example filtered
	    light, emission of specific frequencies
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Soil Temperature">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The temperature of soil manipulated for example through
	    heating or cooling.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Air Temperature">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The air temperature manipulated e.g. thourgh heating or
	    cooling.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Humidity">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Humidity is the amount of water vapor in the air. Water
	    vapor is the gaseous state of water and is invisible
	    ( Wikipedia, https://goo.gl/tcVXHc )
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Water Content">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Water content or moisture content is the quantity of
	    water contained in a material, such as soil (called
	    soil moisture) ( Wikipedia, https://goo.gl/WuyBzY )
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Xenobiotics">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    A xenobiotic is a foreign chemical substance found
	    within an ecosystem that is not normally naturally
	    produced by or expected to be present within that
	    ecosystem. It can also cover substances which are
	    present in much higher concentrations than usual
	    (Wikipedia, https://goo.gl/8RdRfX)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Precipitation">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    In meteorology, precipitation is any product of the
	    condensation of atmospheric water vapour that falls
	    under gravity ( Wikipedia, https://goo.gl/Z6UbIs ).
	    This category captures precipitation no matter if the
	    cause is natural or artificial (e.g. rain simulation).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Nutrient Availability">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any manipulation to the availability of nutrients for
	    consumers as well as producers.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Soil Texture">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Soil texture is a qualitative classification tool used
	    in both the field and laboratory to determine classes
	    for (agricultural) soils based on their physical texture
	    ( Wikipedia, https://goo.gl/dd5XK7 ).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="pH Value">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any pH related manipulation. For example the soil pH.
	    The soil pH is a measure of the acidity or alkalinity
	    in soils. pH is defined as the negative logarithm (base
	    10) of the activity of hydronium ions (H+ or, more
	    precisely, H 3O+ aq) in a solution ( Wikipedia,
	    https://goo.gl/4l1jlp )
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <!-- disturbance -->
      <xs:enumeration value="Management">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any management related manipulation. This includes
	    management for aesthetics, fish, recreation, urban
	    values, water, wilderness, wildlife, wood products,
	    forest genetic resources, and other forest resource
	    values. ( Wikipedia, https://goo.gl/2ViUAX )
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Biomass Removal">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any removal of biomass may it be natural, semi natural
	    or simulated. For example different browsing
	    intentities of deer due to a plot selection or the
	    clipping of plant parts in order to simulated a varying
	    ammount of consumed biomass or the harvesting of crops
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Pathogen Infection">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    A controlled infection with pathogens of whatever kind
	    (e.g. viruses or microorganisms) or the selection of
	    individuals or plots which are affeted by a pathogen.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Fire Disturbance">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any fire related disturbance. For example plot
	    selection creates gradient along areas with different
	    frequency of fire disturbances.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Wind Disturbance">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any wind related disturbance. For example plot
	    selection creates gradient along areas with different
	    frequency of wind disturbances.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Flooding">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Any distubance related to a flooding. For example
	    the plot selection aims for areas that have been
	    flooded at least once in the last 5 years or plots
	    that have a similar set of other factors and the
	    time since the flooding event is used as a gradient.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <!-- topo -->
      <xs:enumeration value="Elevation">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The plot selection creates an elevation gradient. Or
	    plots of a certain elevation only are selected.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Inclination">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    The plot selection creates an inclination gradient.
	    Or plots of a certain inclination are selected only.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <!-- scale -->
      <xs:enumeration value="Time">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Time as an experimental design parameter. This could
	    be for example used if the exposure time of animals
	    or plants to certain radiation has been manipulated
	    in different sets to be compared in the setup.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Space">
	<xs:annotation xml:lang="en">
	  <xs:documentation>
	    Time as an experimental design parameter. For example plots
	    are selected with a specific distance to road to see if dear
	    browsing is affected by disturbances coming from that roads.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <!-- extension -->
      <xs:pattern value="[A-zäöüÄÖÜß ]*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FactorType">
    <xs:restriction base="xs:string">
      <!-- biotic -->
      <xs:enumeration value="Biotic"/>
      <xs:enumeration value="Abiotic"/>
      <xs:enumeration value="Disturbance"/>
      <xs:enumeration value="Topography"/>
      <xs:enumeration value="Temporal Scale"/>
      <xs:enumeration value="Spatial Scale"/>
      <!-- extension -->
      <xs:pattern value="[A-zäöüÄÖÜß ]*"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- organism -->
  <xs:simpleType name="RankAbbreviation">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	The abbreviation of a taxonomic rank as used in (botanical) name strings
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="subgen."/>
      <xs:enumeration value="sect."/>
      <xs:enumeration value="subsp."/>
      <xs:enumeration value="var."/>
      <xs:enumeration value="subvar."/>
      <xs:enumeration value="forma"/>
      <xs:enumeration value="f."/>
      <xs:enumeration value="subforma"/>
      <xs:enumeration value="subf."/>
      <xs:enumeration value="f.spec."/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Insertpoint">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="3"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
