			    <xs:element name="space" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The space facet covers all localization aspects.
					The information ranges from location names over the
					specification of the country or continent down to
					the extent and resolution of the data.
				    </xs:documentation>
				</xs:annotation>

				<xs:complexType>
				    <xs:sequence>
					<xs:element name="is" minOccurs="1" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="covered" minOccurs="1" maxOccurs="1">
							<xs:annotation>
							    <xs:documentation xml:lang="en">
								Space is covred by bounding box, eleveation or and coordinates
							    </xs:documentation>
							</xs:annotation>

							<xs:complexType>
							    <xs:sequence>
								<xs:element name="by" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="boundingBox" minOccurs="0" maxOccurs="1">
										<xs:annotation>
										    <xs:documentation xml:lang="en">
											The bounding box is a three dimensional virtual object in space
											that is made up by the four coordinates (N,E,S,W) in form of decimal
											degree as well as the maximum and minimum altitude (reference sealevel).
										    </xs:documentation>
										</xs:annotation>
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="with" minOccurs="1" maxOccurs="1">
											    <xs:complexType>
												<xs:sequence>
												    <xs:element name="northBoundingCoordinate" type="NorthSouthBoundingCoordinate" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The north most geographic coordinate that signifies the boundary of the data. It
														has to be given in decimal degree between -90 and +90 degree.
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>

												    <xs:element name="southBoundingCoordinate" type="NorthSouthBoundingCoordinate" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The soutch most geographic coordinate that signifies the boundary of the data. It
														has to be given in decimal degree between -90 and +90 degree.
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>

												    <xs:element name="eastBoundingCoordinate" type="EastWestBoundingCoordinate" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The east most geographic coordinate that signifies the boundary of the data. It
														has to be given in decimal degree between -180 and +180 degree.
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>

												    <xs:element name="westBoundingCoordinate" type="EastWestBoundingCoordinate" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The west most geographic coordinate that signifies the boundary of the data. It
														has to be given in decimal degree between -180 and +180 degree.
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="inFormat" type="xs:string" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The format of the bounding box coordinates is decimal degrees
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												</xs:sequence>
											    </xs:complexType>
											</xs:element>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>

									    <xs:element name="boundingElevation" minOccurs="0" maxOccurs="1">
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="with" minOccurs="1" maxOccurs="1">
											    <xs:complexType>
												<xs:sequence>
												    <xs:element name="maximumElevation" type="xs:decimal" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The maximum elevation the data has been taken from.
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="minimumElevation" type="xs:decimal" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The minimum elevation the data has been taken from.
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="elevationUnit" type="LengthUnit" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The unit of the maximum elevation (e.g. Meter, Centimeter)
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="elevationDatum" type="ElevationDatum" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The reference datum of the elevation (e.g. Mean Sealevel)
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												</xs:sequence>
											    </xs:complexType>
											</xs:element>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>
									    <xs:element name="utmCoordinate" minOccurs="1" maxOccurs="1">
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="with" minOccurs="1" maxOccurs="unbounded">
											    <xs:complexType>
											        <xs:sequence>
												    <xs:element name="zone" type="xs:integer" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The numerical zone corresponding to the central meridian
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>

												    <xs:element name="subZone" type="xs:integer" minOccurs="1" maxOccurs="1">
													<xs:annotation>
													    <xs:documentation xml:lang="en">
														The subzone letter corresponding to one of the 20 north-south divisions of the
														UTM grid system
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>

												    <xs:element name="northSouth" type="NorthSouth" minOccurs="1" maxOccurs="1">
