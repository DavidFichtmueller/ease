<?xml version="1.0" encoding="UTF-8"?>

<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
    version="0.0.1"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:sawsdl="http://www.w3.org/ns/sawsdl">

    <xs:element name="cafe">
	<xs:complexType>
	    <xs:sequence>
		<xs:element name="the" minOccurs="1" maxOccurs="1">
		    <xs:complexType>
			<xs:sequence>
			    <xs:element name="physical" minOccurs="1" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The physical element captures important information
					on the referenced search object that is classified
					using the other elements of the schema.
				    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
				    <xs:sequence>
					<xs:element name="object" minOccurs="1" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>

						    <xs:element name="has" minOccurs="1" maxOccurs="1">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="the" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="id" type="xs:integer" minOccurs="1" maxOccurs="1"/>
									    <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
										<xs:annotation>
										    <xs:documentation xml:lang="en">
											The full filename of the object with extension
										    </xs:documentation>
										</xs:annotation>
									    </xs:element>
									    <xs:element name="downloadUrl" type="xs:string" minOccurs="0" maxOccurs="1">
									    </xs:element>
									    <xs:element name="dataType" minOccurs="1" maxOccurs="1">
										<xs:annotation>
										    <xs:documentation xml:lang="en">
											The type of object
											referenced with this
											annotation. This can be
											tabular, image, audio, video
											or any other you specify.
										    </xs:documentation>
										</xs:annotation>
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="tabular" type="xs:boolean" minOccurs="1" maxOccurs="1">
											    <xs:annotation xml:lang="en">
												<xs:documentation>
												    Any tabular data file tpye (e.g. csv, excel)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="image" type="xs:boolean" minOccurs="1" maxOccurs="1">
											    <xs:annotation xml:lang="en">
												<xs:documentation>
												    Any image file tpye (e.g. jpeg)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="audio" type="xs:boolean" minOccurs="1" maxOccurs="1">
											    <xs:annotation xml:lang="en">
												<xs:documentation>
												    Any audio file tpye (e.g. mp3)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="video" type="xs:boolean" minOccurs="1" maxOccurs="1">
											    <xs:annotation xml:lang="en">
												<xs:documentation>
												    Any video file tpye (e.g. mp4, avi)
												</xs:documentation>
											    </xs:annotation>
											</xs:element>
											<xs:element name="other" type="xs:string" minOccurs="1" maxOccurs="1"/>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						    <xs:element name="is" minOccurs="1" maxOccurs="1">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="located" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="at" minOccurs="1" maxOccurs="1">
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="dataCenter" minOccurs="1" maxOccurs="1">
											    <xs:annotation>
												<xs:documentation xml:lang="en">
												    In case of an electronic
												    data type you can set this
												    to the data center the data
												    is hosted at.
												</xs:documentation>
											    </xs:annotation>

											    <xs:complexType>
												<xs:sequence>
												    <xs:element name="sgn" type="xs:boolean" minOccurs="1" maxOccurs="1">
													<xs:annotation xml:lang="en">
													    <xs:documentation>
														Data Center Senckenberg (SGN)
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="bgbm" type="xs:boolean" minOccurs="1" maxOccurs="1">
													<xs:annotation xml:lang="en">
													    <xs:documentation>
														Botanic Garden and Botanical Museum Berlin-Dahlem
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="pangaea" type="xs:boolean" minOccurs="1" maxOccurs="1">
													<xs:annotation xml:lang="en">
													    <xs:documentation>
														PANGAEA: Data Publisher for Earth and Environmental Science
													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												    <xs:element name="other" type="xs:string" minOccurs="1" maxOccurs="1">
													<xs:annotation xml:lang="en">
													    <xs:documentation>
														Any other data center													    </xs:documentation>
													</xs:annotation>
												    </xs:element>
												</xs:sequence>
											    </xs:complexType>
											</xs:element>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>
			    </xs:element>

			    <xs:element name="time" minOccurs="0" maxOccurs="1">
				<xs:annotation>
				    <xs:documentation xml:lang="en">
					The time facet covers all aspects of time
				    </xs:documentation>
				</xs:annotation>

				<xs:complexType>
				    <xs:sequence>
					<xs:element name="frame" minOccurs="1" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="ranges" minOccurs="1" maxOccurs="1">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="from" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="localDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
									    <xs:element name="localTime" type="xs:time" minOccurs="1" maxOccurs="1"/>
									    <xs:element name="inOlsonTimeZone" type="xs:string" minOccurs="1" maxOccurs="1"/>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
								<xs:element name="to" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="localDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
									    <xs:element name="localTime" type="xs:time" minOccurs="1" maxOccurs="1"/>
									    <xs:element name="inOlsonTimeZone" type="xs:string" minOccurs="1" maxOccurs="1"/>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>

						    <xs:element name="is" minOccurs="1" maxOccurs="1">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="characterized" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="by" minOccurs="1" maxOccurs="1">
										<xs:complexType>
										    <xs:sequence>
											<xs:element name="the" minOccurs="1" maxOccurs="1">
											    <xs:complexType>
												<xs:sequence>
												    <xs:element name="geologicalTimePeriod" type="GeologicalTimePeriod" minOccurs="1" maxOccurs="unbounded"/>
												</xs:sequence>
											    </xs:complexType>
											</xs:element>
										    </xs:sequence>
										</xs:complexType>
									    </xs:element>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>

					<xs:element name="setup" minOccurs="1" maxOccurs="1">
					    <xs:complexType>
						<xs:sequence>
						    <xs:element name="adheres" minOccurs="1" maxOccurs="1">
							<xs:complexType>
							    <xs:sequence>
								<xs:element name="to" minOccurs="1" maxOccurs="1">
								    <xs:complexType>
									<xs:sequence>
									    <xs:element name="temporalExtent" type="TimeCategory" minOccurs="1" maxOccurs="1"/>
									    <xs:element name="temporalResolution" type="TimeCategory" minOccurs="1" maxOccurs="1"/>
									</xs:sequence>
								    </xs:complexType>
								</xs:element>
							    </xs:sequence>
							</xs:complexType>
						    </xs:element>
						</xs:sequence>
					    </xs:complexType>
					</xs:element>
				    </xs:sequence>
				</xs:complexType>

