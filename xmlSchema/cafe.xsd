									    </xs:sequence>
									</xs:complexType>
								    </xs:element>

								    <xs:element name="hydrosphere" minOccurs="0" maxOccurs="1">
									<xs:complexType>
									    <xs:sequence>
										<xs:element name="river" minOccurs="0" maxOccurs="unbounded">
										    <xs:complexType>
											<xs:sequence>
											    <xs:element name="zone" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
												    <xs:sequence>
													<xs:element name="crenon" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="rhitron" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="potamon" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="floodplain" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="bank" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="water" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="bed" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
												    </xs:sequence>
												</xs:complexType>
											    </xs:element>
											</xs:sequence>
										    </xs:complexType>
										</xs:element>

										<xs:element name="lake" minOccurs="0" maxOccurs="unbounded">
										    <xs:complexType>
											<xs:sequence>
											    <xs:element name="zone" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
												    <xs:sequence>
													<xs:element name="litoral" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="benthic" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="profundal" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="epilimnion" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="metalimnion" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="hypolimnion" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
													<xs:element name="pelagic" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
												    </xs:sequence>
												</xs:complexType>
											    </xs:element>
											</xs:sequence>
										    </xs:complexType>
										</xs:element>

										<xs:element name="sea" minOccurs="0" maxOccurs="unbounded">
