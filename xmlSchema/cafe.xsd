    <xs:simpleType name="SpatialClass">
	<xs:restriction base="xs:string">
	    <xs:enumeration value="Point">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Defined as: m^2
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	    <xs:enumeration value="Plot">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Defined as: 1 m^2 - 0.01 km^2
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	    <xs:enumeration value="Region">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Defined as: 0.01 km^2 - 10.000 km^2
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	    <xs:enumeration value="Continent">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Defined as: 10.000 km^2 - 100.000.000 km^2
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	    <xs:enumeration value="Global">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Defined as: > 100.000.000 km^2 
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	</xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ControlFactorDependency">
	<xs:restriction base="xs:string">
	    <xs:enumeration value="Independent">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Fully controlled lab experiment that could take place
			anywhere.
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	    <xs:enumeration value="Partially">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Unique features of spatial context partially
			uncontrolled, e.g. light conditions in greenhouse, open
			top chambers.
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	    <xs:enumeration value="Fully">
		<xs:annotation>
		    <xs:documentation xml:lang="en">
			Field studies.
		    </xs:documentation>
		</xs:annotation>
	    </xs:enumeration>
	</xs:restriction>
    </xs:simpleType>

